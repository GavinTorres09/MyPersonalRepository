    }
//Stk.push

Stack <String> stk = new Stack<>();

       stk.push("White Notebook");
       stk.push("Red Notebook");
       stk.push("Black Notebook");

       stk.pop();
       System.out.println(stk);

//Queue
            Queue<String> HotelRoomreservation = new LinkedList<>();
            HotelRoomreservation.add("Patrice");
            HotelRoomreservation.add("Chelsea");
            HotelRoomreservation.add("Trisha");
            HotelRoomreservation.add("Shiena");
            HotelRoomreservation.add("Audrey");
            
            
            System.out.println("The Queue is: " + HotelRoomreservation);
            HotelRoomreservation.remove();

            System.out.println("The Queue is: " + HotelRoomreservation);



    }
}
